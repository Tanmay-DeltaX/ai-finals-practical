weight = float(input("Enter the weight of the man in kg"))
height = float(input("Enter the height of the man in meters"))
bmi = weight/(height**2)

if bmi<18.5:
    print("You are Underweight")
elif bmi>=18.5 and bmi<24.9:
    print("You are normal")
elif bmi>=24.9 and bmi<30:
    print("You are overweight")
else:
    print("You are obese")
